{
    "name": "swiftmade/blogdown",
    "description": "Simple Laravel blog package using markdown",
    "authors": [
        {
            "name": "Ahmet Ozisik",
            "email": "ozisikahmet@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "6.*|7.*|8.*",
        "graham-campbell/markdown": "^13.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "laravel/laravel": "8.*",
        "mockery/mockery": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "autoload": {
        "psr-4": {
            "Swiftmade\\Blogdown\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/IntegrationTestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swiftmade\\Blogdown\\BlogdownProvider"
            ]
        }
    }
}
