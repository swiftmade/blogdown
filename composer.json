{
    "name": "swiftmade/blogdown",
    "description": "Simple Laravel blog package using markdown",
    "authors": [
        {
            "name": "Ahmet Ozisik",
            "email": "ozisikahmet@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "ultrono/markdown": "^14.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laravel/laravel": "7.*|8.*|9.*|10.*",
        "mockery/mockery": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "Swiftmade\\Blogdown\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/IntegrationTestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swiftmade\\Blogdown\\BlogdownProvider"
            ]
        }
    }
}